{
  "name": "monorepo",
  "version": "0.1.0",
  "private": true,
  "workspaces": [
    "packages/@huiji/*",
    "packages/@rimtrans/*"
  ],
  "bin": "packages/@rimtrans/service/dist/cjs/main.js",
  "pkg": {
    "assets": [
      "packages/@rimtrans/core/About/*",
      "packages/@rimtrans/core/Defs/**/*",
      "packages/@rimtrans/core/Languages/**/*",
      "packages/@rimtrans/core/manifest.json",
      "packages/@rimtrans/ui/dist/**/*"
    ]
  },
  "scripts": {
    "dep": "yarn ts scripts/dependencies.ts",
    "clean:dist": "rimraf dist && lerna run clean:dist",
    "gen-files": "lerna run gen-files",
    "env": "lerna run env",
    "build:pkg": "pkg . -t node10-linux-x64,node10-macos-x64,node10-win-x64 -o dist/rimtrans",
    "build": "lerna run build && yarn build:pkg",
    "lint": "lerna run lint",
    "test": "lerna run test",
    "ts": "ts-node -r tsconfig-paths/register -P scripts/tsconfig.json"
  },
  "devDependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.15",
    "@fortawesome/free-brands-svg-icons": "^5.7.2",
