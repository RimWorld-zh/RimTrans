    "@fortawesome/vue-fontawesome": "^0.1.5",
    "@huiji/graphql-tools": "^0.3.1",
    "@huiji/shared-utils": "^1.0.0",
    "@huiji/tslint-rules": "^1.0.2",
    "@huiji/vue-cli-utils": "^0.1.1",
    "@huiji/vue-silo": "^0.1.0",
    "@types/compression": "^0.0.36",
    "@types/dateformat": "^3.0.0",
    "@types/execa": "^0.9.0",
    "@types/express": "^4.16.1",
    "@types/globby": "^8.0.0",
    "@types/jest": "^24.0.5",
    "@types/js-yaml": "^3.12.0",
    "@types/json5": "^0.0.30",
    "@types/lru-cache": "^4.1.1",
    "@types/mkdirp": "^0.5.2",
    "@types/ncp": "^2.0.1",
    "@types/node": "^10.12.26",
    "@types/rimraf": "^2.0.2",
    "@types/serve-favicon": "^2.2.30",
